{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "public/",
    "outDir": "public/dist",
    "paths": {
      "@grafana/slate-react": [
        "../.yarn/cache/@types-slate-react-npm-0.22.5-0d49b63254-afc83c7f47.zip/node_modules/@types/slate-react/index.d.ts"
      ]
    },
    "rootDirs": ["public/"],
    "allowJs": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false
  },
  "extends": "@grafana/tsconfig/base.json",
  "include": [
    "public/app/**/*.ts*",
    "public/e2e-test/**/*.ts",
    "public/test/**/*.ts",
    "public/vendor/**/*.ts",
    "packages/jaeger-ui-components/typings",
    "packages/grafana-data/typings"
  ]
}
